  object QPedidos: TADOQuery
    Active = True
    Connection = Conexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT'
      #9'PED.NRDOC_S,'
      #9'PES.DES_S,'
      #9'P.DES_S,'
      #9'PED.TOTQTDE_F,'
      #9'PED.TOTALLIQUIDO,'
      #9'E.DES_S,'
      #9'PED.DATEMI_D,'
      #9'MP.PREUNIT_F,'
      #9'MP.QTDE_F,'
      #9'UN.DES_S,'
      #9'PES.CEL_S,'
      #9'PED.TIPOVENDA,'
      #9'PAG.DES_S,'
      #9'PED.OBSINTERNA,'
      #9'CP.DES_S,'
      #9'S.DES_S,'
      #9'S.PREVENDA_F,'
      #9'MP.PRETOTALD_F'
      'FROM MOVIESTOP AS PED'
      'LEFT JOIN CADPES AS PES ON PED.CODPES_I = PES.COD_I'
      'LEFT JOIN PRODMOVI AS PM ON PM.NRDOC_S = PED.NRDOC_S'
      'LEFT JOIN CADEMP AS E ON PED.CODEMP_I = E.COD_I'
      'LEFT JOIN PRODMOVIP AS MP ON MP.NRDOC_S = PED.NRDOC_S'
      'LEFT JOIN CADPRO AS P ON P.COD_I = MP.CODPRO_I'
      'LEFT JOIN CADUNI AS UN ON UN.COD_I = MP.CODUNID'
      'LEFT JOIN CADFPGTO AS PAG ON PAG.CODIGO = PED.CODFPGTO_I'
      'LEFT JOIN  CADCPGTO  AS CP ON PAG.CODIGO = CP.COD_I'
      'LEFT  JOIN CADSERV AS S ON MP.CODPRO_i = S.COD_I'
      'WHERE PED.NRDOC_S = '#39'P01-006824'#39';'
      ''
      ''
      ''
      ''
      ''
      '')
